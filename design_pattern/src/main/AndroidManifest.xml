<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="alidoran.design_pattern.java.memento.MementoActivity"
            android:exported="false" />
        <activity android:name="alidoran.design_pattern.java.adapter.AdapterMain" />
        <activity android:name="alidoran.design_pattern.java.prototype.MainPrototype" />
        <activity android:name="alidoran.design_pattern.java.builder.MainBuilderActivity" />
        <activity android:name="alidoran.design_pattern.java.factory_method.MainFactoryMethodActivity" />
        <activity android:name="alidoran.design_pattern.java.simple_factory.Main_SimpleFactory" />
        <activity
            android:name="alidoran.DesignPatternActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>